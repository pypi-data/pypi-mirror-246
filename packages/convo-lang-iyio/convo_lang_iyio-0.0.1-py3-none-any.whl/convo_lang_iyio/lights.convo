# Turn the lights in the user's house on or off
> turnOnOffLights(
    # The state to set the lights to
    state: enum("on" "off")
) -> (

    @shared
    fromFn=state

    // setPinHigh would be defined as an extern function written in another language
    return(setPinHigh(eq(state "on")))

    //return({state:state})
)

