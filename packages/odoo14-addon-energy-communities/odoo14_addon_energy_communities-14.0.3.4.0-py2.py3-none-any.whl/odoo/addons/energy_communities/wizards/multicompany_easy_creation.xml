<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="wizard_multicompany_easy_creation_form_inherit" model="ir.ui.view">
            <field
        name="name"
      >account_multicompany_easy_creation.wizard_multicompany_easy_creation_form_inherit</field>
            <field name="model">account.multicompany.easy.creation.wiz</field>
            <field
        name="inherit_id"
        ref="account_multicompany_easy_creation.wizard_multicompany_easy_creation_view_form"
      />
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <xpath expr="//page" position="before">
                    <page string="Cooperator">
                        <group string="Cooperator">
                            <field name="property_cooperator_account" />
                            <field name="product_share_template" />
                            <field name="capital_share" />
                            <field name="create_user" />
                        </group>
                    </page>
                </xpath>
                <xpath expr="//page" position="before">
                    <page string="Energy Community">
                        <group string="Energy Community">
                            <field name="parent_id" required="1" />
                            <field name="vat" />
                            <field name="state_id" required="1" />
                            <field name="city" required="1" />
                            <field name="street" required="1" />
                            <field name="zip_code" required="1" />
                            <field name="foundation_date" />
                            <field name="email" required="1" />
                            <field name="legal_form" />
                            <field name="legal_name" />
                            <field name="ce_status" />
                            <field name="phone" />
                            <field name="default_lang_id" required="1" />
                            <field name="website" />
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
